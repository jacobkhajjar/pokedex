// @ts-nocheck

{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "Node16",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
